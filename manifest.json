{
    "name": "Pink Fluffy Trello",
    "version": "1.0",
    "description": "Turns your trello background into pink fluff",
    "permissions": ["activeTab", "declarativeContent", "storage", "*://*/*"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": {
        "16": "images/fluff16.png",
        "32": "images/fluff32.png",
        "48": "images/fluff48.png",
        "128": "images/fluff128.png"
    },
    "manifest_version": 2
}